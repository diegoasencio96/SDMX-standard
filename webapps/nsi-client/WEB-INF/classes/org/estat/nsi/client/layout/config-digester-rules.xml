<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE digester-rules PUBLIC
        "-//Jakarta Apache //DTD digester-rules XML V1.0//EN"
        "digester-rules.dtd">

<digester-rules>
    <pattern value="results-layout">
        <object-create-rule classname="java.util.ArrayList"/>

        <pattern value="KeyFamily">
            <object-create-rule classname="org.estat.nsi.client.layout.KeyFamilyCfg"/>
            <set-properties-rule/>
            <set-next-rule methodname="add"/>
            <pattern value="Component">
                <object-create-rule classname="org.estat.nsi.client.layout.KeyFamilyComponentCfg"/>
                <set-properties-rule>
                    <alias attr-name="defaultAxis" prop-name="defaultAxisAsString"/>
                </set-properties-rule>
                <set-next-rule methodname="addComponent"/>
            </pattern>
        </pattern>
    </pattern>
</digester-rules>
